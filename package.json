{
  "name": "workflow-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "npm run typecheck && eslint ./ --max-warnings=40 && echo '' && echo '\n\\033[0;32m✔ Your .js files look good\\033[0m' && echo '' ",
    "lint:fix": "npm run typecheck && eslint ./ --max-warnings=40 --fix && echo '' && echo '\n\\033[0;32m✔ Your .js files look good\\033[0m' && echo '' ",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
    "test": "jest",
    "typecheck": "tsc --strict"
  },
  "dependencies": {
    "gojs": "^2.3.11",
    "gojs-react": "^1.1.2",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.5",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-plugin-react": "latest",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "latest",
    "ts-jest": "^29.1.1",
    "typescript": "latest"
  },
  "engines": {
    "node": "^16.0"
  }
}
